    "text": "Une seule chose à la fois : le reste peut attendre.",
    "author": null,
    "need": "calme",
    "mood": null,
    "tone": "neutre",
    "energy": 1,
    "length": "courte",
    "is_injunctive": false,
    "is_guilt_inducing": false,
    "is_toxic_positive": false,
    "language": "fr"
  },
  {
    "id": "cit_0007",
    "text": "Ce qui est urgent n’est pas toujours important.",
    "author": null,
    "need": "calme",
    "mood": "stressé",
    "tone": "stoïque",
    "energy": 2,
    "length": "courte",
    "is_injunctive": false,
    "is_guilt_inducing": false,
    "is_toxic_positive": false,
    "language": "fr"
  },
  {
