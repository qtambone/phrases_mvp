    "need": "calme",
    "mood": null,
    "tone": "poétique",
    "energy": 1,
    "length": "courte",
    "is_injunctive": false,
    "is_guilt_inducing": false,
    "is_toxic_positive": false,
    "language": "fr"
  },
  {
