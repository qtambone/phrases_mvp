    "need": "réconfort",
    "mood": "stressé",
    "tone": "neutre",
    "energy": 1,
    "length": "courte",
    "is_injunctive": false,
    "is_guilt_inducing": false,
    "is_toxic_positive": false,
    "language": "fr"
  },
  {
    "id": "cit_0020",
    "text": "Tu peux demander moins, et respirer plus.",
    "author": null,
    "need": "réconfort",
    "mood": null,
    "tone": "direct",
    "energy": 1,
    "length": "courte",
    "is_injunctive": false,
    "is_guilt_inducing": false,
    "is_toxic_positive": false,
    "language": "fr"
  },
  {
    "id": "cit_0021",
    "text": "Si tu as besoin de douceur, ce n’est pas une faiblesse.",
    "author": null,
    "need": "réconfort",
    "mood": "triste",
    "tone": "poétique",
    "energy": 1,
