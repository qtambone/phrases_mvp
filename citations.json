    "need": "réconfort",
    "mood": "neutre",
    "tone": "accompagnant",
    "energy": 1,
    "length": "courte",
    "is_injunctive": false,
    "is_guilt_inducing": false,
    "is_toxic_positive": false,
    "language": "fr"
  },
  {
    "id": "cit_0015",
    "text": "Tu n’es pas en retard sur ta vie : tu es en train de la vivre.",
    "author": null,
    "need": "réconfort",
    "mood": null,
    "tone": "stoïque",
    "energy": 2,
    "length": "courte",
    "is_injunctive": false,
    "is_guilt_inducing": false,
    "is_toxic_positive": false,
    "language": "fr"
  },
  {
    "id": "cit_0016",
    "text": "Il y a des jours où tenir est déjà courageux.",
    "author": null,
    "need": "réconfort",
    "mood": "fatigué",
    "tone": "accompagnant",
    "energy": 1,
    "length": "courte",
    "is_injunctive": false,
    "is_guilt_inducing": false,
    "is_toxic_positive": false,
    "language": "fr"
  },
  {
    "id": "cit_0017",
    "text": "Tu peux être fatigué·e et digne, en même temps.",
    "author": null,
    "need": "réconfort",
    "mood": "fatigué",
    "tone": "neutre",
    "energy": 1,
    "length": "courte",
    "is_injunctive": false,
    "is_guilt_inducing": false,
    "is_toxic_positive": false,
    "language": "fr"
  },
  {
