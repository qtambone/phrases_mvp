    "need": "clarté",
    "mood": null,
    "tone": "direct",
    "energy": 3,
    "length": "courte",
    "is_injunctive": false,
    "is_guilt_inducing": false,
    "is_toxic_positive": false,
    "language": "fr"
  },
  {
