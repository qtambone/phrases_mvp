    "mood": "stressé",
    "tone": "direct",
    "energy": 2,
    "length": "courte",
    "is_injunctive": false,
    "is_guilt_inducing": false,
    "is_toxic_positive": false,
    "language": "fr"
  },
  {
    "id": "cit_0026",
    "text": "La clarté arrive souvent après avoir simplifié.",
    "author": null,
    "need": "clarté",
    "mood": null,
    "tone": "stoïque",
    "energy": 1,
    "length": "courte",
    "is_injunctive": false,
    "is_guilt_inducing": false,
    "is_toxic_positive": false,
    "language": "fr"
  },
  {
    "id": "cit_0027",
    "text": "Tu n’as pas besoin d’une solution parfaite, juste d’un prochain pas clair.",
