    "energy": 1,
    "length": "courte",
    "is_injunctive": false,
    "is_guilt_inducing": false,
    "is_toxic_positive": false,
    "language": "fr"
  },
  {
    "id": "cit_0011",
    "text": "Une journée chargée peut contenir un moment léger.",
    "author": null,
    "need": "calme",
    "mood": null,
    "tone": "neutre",
    "energy": 2,
    "length": "courte",
    "is_injunctive": false,
    "is_guilt_inducing": false,
    "is_toxic_positive": false,
    "language": "fr"
  },
  {
    "id": "cit_0012",
    "text": "Un esprit tranquille se fabrique par petites respirations.",
    "author": null,
    "need": "calme",
    "mood": null,
    "tone": "accompagnant",
    "energy": 1,
    "length": "courte",
    "is_injunctive": false,
    "is_guilt_inducing": false,
    "is_toxic_positive": false,
    "language": "fr"
  },
  {
