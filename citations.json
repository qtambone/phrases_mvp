    "language": "fr"
  },
  {
    "id": "cit_0002",
    "text": "Le calme n’est pas l’absence de bruit ; c’est l’espace à l’intérieur.",
    "author": null,
    "need": "calme",
    "mood": "stressé",
    "tone": "stoïque",
    "energy": 1,
    "length": "courte",
    "is_injunctive": false,
    "is_guilt_inducing": false,
    "is_toxic_positive": false,
    "language": "fr"
  },
  {
