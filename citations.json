    "mood": null,
    "tone": "neutre",
    "energy": 2,
    "length": "courte",
    "is_injunctive": false,
    "is_guilt_inducing": false,
    "is_toxic_positive": false,
    "language": "fr"
  },
  {
    "id": "cit_0028",
    "text": "Ce qui t’épuise le plus, c’est parfois l’indécision.",
    "author": null,
    "need": "clarté",
    "mood": "stressé",
    "tone": "stoïque",
    "energy": 2,
    "length": "courte",
    "is_injunctive": false,
    "is_guilt_inducing": false,
    "is_toxic_positive": false,
    "language": "fr"
  },
  {
    "id": "cit_0029",
    "text": "Un choix imparfait peut valoir mieux qu’une attente infinie.",
